// --------------------------------------- Alma

class Alma {

    // -------------------- Referencias

    const dineroAlMorir
    const cantAccionesBuenas

    // -------------------- Metodos

    // Accessors

    method cantAccionesBuenas() = cantAccionesBuenas

    // Consulta

    method capital() = dineroAlMorir + cantAccionesBuenas

    method puedeCostearlo(paquete) = self.capital() > paquete.costo(self)
 
}